{
  "name": "too",
  "version": "2.0.3",
  "description": "Combine multiple processes' Stdout/Stderr/SIGINT to keep them all foreground",
  "scripts": {
    "build": "tsc",
    "validate": "npm run lint && npm run build && npm link && too -c 'echo foo' -c 'echo bar'",
    "lint": "tslint ./src/**/*.ts",
    "test": "jest"
  },
  "bin": {
    "too": "dist/bin/too.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/otiai10/too.js.git"
  },
  "author": "otiai10",
  "license": "MIT",
  "keywords": [
    "Command",
    "Parallel",
    "SIGINT",
    "ctrl+c"
  ],
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^26.0.4",
    "@types/node": "^14.0.23",
    "codecov": "^3.7.0",
    "jest": "^26.1.0",
    "lookpath": "^1.1.0",
    "ts-jest": "^26.1.3",
    "tslint": "^6.1.2",
    "typescript": "^3.9.7"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "./src/lib/*.ts"
    ],
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  }
}
